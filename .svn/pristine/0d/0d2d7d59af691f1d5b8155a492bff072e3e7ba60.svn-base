.el-collapse{
    border: 1px solid rgb(236, 223, 223);
    border-radius: 0;
}

.el-collapse-item{}

.el-collapse-item:last-child{
    margin-bottom: -1px;
}

.el-collapse-item.is-active > .el-collapse-item__header .el-collapse-item__header__arrow{
    transform: rotate(90deg);
}

.el-collapse-item__header{
    height: 43px;
    line-height: 43px;
    padding-left: 15px;
    background-color: #fff;
    color: #555;
    cursor: pointer;
    border-bottom: 1px solid rgb(236, 223, 223);
    font-size: 13px;
}

.el-collapse-item__header__arrow{
    margin-right: 8px;
    transition: transform .3s;
}

.el-collapse-item__wrap{
    will-change: height;
    background-color: rgb(251, 254, 253);
    overflow: hidden;
    box-sizing: border-box;
    border-bottom: 1px solid rgb(236, 223, 223);
}

.el-collapse-item__content{
    padding: 10px 15px;
    font-size: 13px;
    color: #000;
    line-height: 1.769230769230769;
}
