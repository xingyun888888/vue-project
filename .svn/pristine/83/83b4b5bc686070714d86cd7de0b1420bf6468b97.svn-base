.container-body{
  .box-div{
    div{
      display:inline-block;
    }
  }
  .table{
    display:block;
    margin-top:30px;
  }
}
.custodian{
    p{
      margin:0;
      width:85%;
      border-bottom:1px dashed #F1F1F1;
      &:nth-child(1){
        border-top:1px solid #4E88D8;
      }
      span{
        display:inline-block;
        height:35px;
        line-height:35px;
        &:nth-child(1){
          width:150px;
          text-align: right;
          margin-right:20px;
        }
      }
    }
  .getBack{
    margin:15px 0 0 20px;
  }
}

